 <script>
    $('.btnadminPage').click(function(){
      $(this).toggleClass("click");
      $('.sidebaradminPage').toggleClass("show");
    });
      $('.feat-btn').click(function(){
        $('.sidebaradminPage ul .feat-show').toggleClass("show");
        $('.sidebaradminPage ul .first').toggleClass("rotate");
      });
      $('.serv-btn').click(function(){
        $('.sidebaradminPage ul .serv-show').toggleClass("show1");
        $('.sidebaradminPage ul .second').toggleClass("rotate");
      });
      $('.sidebaradminPage ul li').click(function(){
        $(this).addClass("active").siblings().removeClass("active");
      });
 </script>